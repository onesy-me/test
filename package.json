{
  "name": "@onesy/test",
  "version": "1.0.0",
  "description": "Test suite for front end and back end",
  "repository": "https://github.com/onesy-me/test.git",
  "author": "Lazar EriÄ‡ <lazareric1@proton.me>",
  "license": "MIT",
  "private": false,
  "main": "./src/index.ts",
  "bin": {
    "onesy-test": "./cli.js"
  },
  "keywords": [
    "test",
    "tests",
    "testing",
    "test-framework",
    "testing-framework",
    "testing-library",
    "testing",
    "expect",
    "assert",
    "web",
    "frontend",
    "front-end",
    "backend",
    "back-end",
    "web",
    "browser",
    "javascript",
    "js",
    "typescript",
    "express",
    "expressjs",
    "nodejs",
    "node",
    "library",
    "onesy",
    "utils"
  ],
  "scripts": {
    "prebuild": "rimraf build",
    "build": "node utils/js/build.js",
    "release": "yarn build && yarn version --patch --no-git-tag-version && yarn build && cd ./build && npm publish",
    "release-pure": "yarn build && yarn build && cd ./build && npm publish",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "type": "tsc --noEmit -p tsconfig.json",
    "cov": "cross-env NODE_ENV=test mocha",
    "test:pre": "pm2 update && pm2 delete all && pm2 start utils/js/test/app.js && wait-on http://localhost:4000",
    "test:post": "pm2 delete all",
    "test": "yarn lint && yarn type && yarn build && yarn test:pre && yarn cov && yarn test:post"
  },
  "dependencies": {
    "@onesy/date": "^1.0.0",
    "@onesy/diff": "^1.0.0",
